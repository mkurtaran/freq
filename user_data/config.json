{
  "dry_run": true,
  "dry_run_wallet": 1000,

  "stake_currency": "USDT",
  "stake_amount": 50,

  "tradable_balance_ratio": 0.99,

  "fiat_display_currency": "USD",

  "timeframe": "5m",

  "strategy": "NewStrategy",

  "freqai": {
    "enabled": true,
    "identifier": "my_model",

    "train_period_days": 30,
    "backtest_period_days": 7,
    "live_retrain_hours": 0,

    "data_split_parameters": {
      "test_size": 0.2,
      "shuffle": false
    },

    "feature_parameters": {
      "include_timeframes": ["5m"],
      "include_corr_pairlist": [],
      "indicator_periods_candles": [5, 10, 14, 21, 50],
      "label_period_candles": 10
    }
  },

  "exchange": {
    "name": "binance",
    "key": "",
    "secret": "",
    "ccxt_config": {
      "enableRateLimit": true
    },
    "ccxt_async_config": {
      "enableRateLimit": true
    },
    "pair_whitelist": [
      "BTC/USDT",
      "ETH/USDT"
    ],
    "pair_blacklist": [
      "BNB/USDT"
    ]
  },

  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],

  "entry_pricing": {
    "price_side": "same",
    "use_order_book": false
  },

  "exit_pricing": {
    "price_side": "same",
    "use_order_book": false
  },

  "order_types": {
    "entry": "limit",
    "exit": "limit",
    "stoploss": "market",
    "stoploss_on_exchange": false
  },

  "order_time_in_force": {
    "entry": "GTC",
    "exit": "GTC"
  },

 "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "verbosity": "error",
    "enable_openapi": true,
    "username": "admin",
    "password": "!!mert"
  },

  "internals": {
    "process_throttle_secs": 5
  }
}
